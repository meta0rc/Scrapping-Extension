{
    "manifest_version": 2,
    "name": "Scrapping",
    "description": "Scrapping urls",
    "version": "0.1",
    "browser_action": {
        "default_title": "Scrapping",
        "default_icon": "logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}